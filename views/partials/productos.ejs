
<div class="card" id="<%= productos.id %>">
  <div class="imagen">
    <img src="<%= productos.imagen %>" alt="<%= productos.id %>">
  </div>
  <div class="card-body">
    <h1><%= productos.nombre %></h1>
    <p><%= productos.descripcion %></p>
    <p>$ <%= productos.precio %></p>
    <a href="/productos/:id" class="btn btn-primary">Ver Detalles</a>
    <a href="/cart.html" class="btn btn-primary">Comprar</a>
  </div>
</div>
 <br>